        "## Q4: Save the following dataframe content to a CSV file (data.csv) in your downloads folder"
      ],
      "metadata": {
        "id": "BCWITkmopblI"
      }
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "r3THwBOGpP9F"
      },
      "outputs": [],
      "source": [
        "import pandas as pd\n",
        "\n",
        "data = {\"Id\": [1, 2, 3],\n",
        "        \"Name\": [\"Alice\", \"Bob\", \"Charlie\"],\n",
        "        \"Subject\": [\"Science\", \"Maths\", \"History\"]}\n",
        "df = pd.DataFrame(data)"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import os\n",
        "\n",
        "\n",
        "data = {\n",
        "    \"Id\": [1, 2, 3],\n",
        "    \"Name\": [\"Alice\", \"Bob\", \"Charlie\"],\n",
        "    \"Subject\": [\"Science\", \"Maths\", \"History\"]\n",
        "}\n",
        "\n",
        "df = pd.DataFrame(data)\n",
        "\n",
        "downloads_folder = os.path.join(os.path.expanduser(\"~\"), \"Downloads\")\n",
        "os.makedirs(downloads_folder, exist_ok=True)\n",
        "\n",
        "\n",
        "file_path = os.path.join(downloads_folder, \"data.csv\")\n",
        "\n",
        "\n",
        "df.to_csv(file_path, index=False)\n",
        "\n",
        "print(f\"data.csv successfully saved to: {file_path}\")\n"
      ],
      "metadata": {
        "id": "Joc7XafnntaG",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 0
        },
        "outputId": "89723161-889f-456e-d4b3-99665604c797"
      },
      "execution_count": 9,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "data.csv successfully saved to: /root/Downloads/data.csv\n"
          ]
        }
      ]
    },
