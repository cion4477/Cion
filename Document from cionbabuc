        "import numpy as np\n",
        "\n",
        "\n",
        "random_array = np.random.rand(10, 10)\n",
        "\n",
        "\n",
        "np.save(\"random_array.npy\", random_array)\n",
        "\n",
        "print(\"random_array.npy file has been saved successfully.\")\n"
      ],
      "metadata": {
