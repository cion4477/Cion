        "## Q5: Save the above dataframe content to an Excel (data.xlsx, sheet name: Sheet1) file in your documents folder"
      ],
      "metadata": {
        "id": "VF0nJjGElq0A"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import os\n",
        "\n",
        "\n",
        "data = {\n",
        "    \"Id\": [1, 2, 3],\n",
        "    \"Name\": [\"Alice\", \"Bob\", \"Charlie\"],\n",
        "    \"Subject\": [\"Science\", \"Maths\", \"History\"]\n",
        "}\n",
        "df = pd.DataFrame(data)\n",
        "\n",
        "r\n",
        "documents_folder = os.path.join(os.path.expanduser(\"~\"), \"Documents\")\n",
        "os.makedirs(documents_folder, exist_ok=True)\n",
        "\n",
        "\n",
        "file_path = os.path.join(documents_folder, \"data.xlsx\")\n",
        "\n",
        "\n",
        "df.to_excel(file_path, index=False, sheet_name=\"Sheet1\")\n",
        "\n",
        "print(f\"Excel file successfully saved to: {file_path}\")\n"
      ],
      "metadata": {
        "id": "ZtOFb7ZJnu1m",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 0
        },
        "outputId": "3f1d433a-dc01-4b91-fc47-6018e3a3fb83"
      },
      "execution_count": 10,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
